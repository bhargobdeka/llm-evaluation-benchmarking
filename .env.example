# LLM Multi-Model Evaluation Framework
# Copy this file to `.env` and set values locally.
# Never commit `.env` files with real keys.

# Provider API keys (active providers in early phases)
ANTHROPIC_API_KEY=
OPENAI_API_KEY=
GEMINI_API_KEY=

# Optional provider keys for later phases
GROQ_API_KEY=

# Optional local endpoint for future local provider support
LOCAL_MODEL_BASE_URL=http://localhost:11434

# Global execution controls (can be overridden by configs/policy.yaml)
LLM_EVAL_MAX_USD_PER_RUN=5
LLM_EVAL_MAX_PARALLEL_REQUESTS=3
LLM_EVAL_STOP_ERROR_RATE_PERCENT=10
